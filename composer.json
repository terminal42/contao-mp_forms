{
  "name": "terminal42/contao-mp_forms",
  "description": "mp_forms extension for Contao Open Source CMS",
  "keywords": ["contao", "forms", "multi", "multipage"],
  "type": "contao-module",
  "license": "LGPL-3.0+",
  "authors":[
    {
      "name":"terminal42 gmbh",
      "homepage":"http://terminal42.ch"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "contao/core-bundle": "^4.9",
    "contao-community-alliance/composer-plugin": "^2.4 || ^3.0",
    "codefog/contao-haste": "^4.18",
    "symfony/var-dumper": "^4.4 || ^5.0",
    "symfony/property-access": "^4.4 || ^5.0"
  },
  "autoload": {
    "files": [
      "FormMPFormPageSwitch.php",
      "FormMPFormPlaceholder.php",
      "MPForms.php",
      "MPFormsFormManager.php",
      "MPFormsStepsModule.php"
    ]
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/mp_forms"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "contao-community-alliance/composer-plugin": true,
      "contao-components/installer": true,
      "composer/package-versions-deprecated": true,
      "ocramius/package-versions": true
    }
  }
}
